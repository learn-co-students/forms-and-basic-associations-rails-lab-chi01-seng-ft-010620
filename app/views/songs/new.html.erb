<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name, :Artist %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :genre_id, :Genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <input id="song_notes_attributes_0_content" name="song[notes_attributes][]"><br>
  <input id="song_notes_attributes_1_content" name="song[notes_attributes][]">

  <%= f.submit %>
<% end %>


